{
    "search.exclude": {
        "**/node_modules": true,
        "dist": true,
        "yarn.lock": true
    },
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/node_modules/*/**": true,
        "dist": true,
        "yarn.lock": true
    },
    "path-intellisense.mappings": {
        "@": "${workspaceRoot}/src",
        "utils": "${workspaceRoot}/src/utils"
    }
}
